<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
 <navigation-rule>
  <display-name>password</display-name>
  <navigation-case>
   <from-outcome>password</from-outcome>
   <to-view-id>/pages/password.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>success</from-outcome>
   <to-view-id>/pages/main.xhtml</to-view-id>
   <redirect include-view-params="true"/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>failed</from-outcome>
   <to-view-id>/pages/loginFailed.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>logout</from-outcome>
   <to-view-id>/pages/welcome.jsp</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>advantaged</from-outcome>
   <to-view-id>/pages/advantaged.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>transfer</from-outcome>
   <to-view-id>/pages/transfer.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>payment</from-outcome>
   <to-view-id>/pages/payment.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>voucher</from-outcome>
   <to-view-id>/pages/voucher.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>vouchers</from-outcome>
   <to-view-id>/pages/vouchers.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>statement</from-outcome>
   <to-view-id>/pages/statement.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <validator>
  <validator-id>advantagedAccountNumberValidator</validator-id>
  <validator-class>br.com.internetbanking.validator.AdvantagedAccountNumber</validator-class>
 </validator>
 <validator>
  <validator-id>scheduleDateValidator</validator-id>
  <validator-class>br.com.internetbanking.validator.ScheduleDateValidator</validator-class>
 </validator>
 <validator>
  <validator-id>expirationDateValidator</validator-id>
  <validator-class>br.com.internetbanking.validator.ExpirationDateValidator</validator-class>
 </validator>
 <application>
  <!-- Locale configuration  -->
  <locale-config>
   <default-locale>en</default-locale>
   <supported-locale>en</supported-locale>
   <supported-locale>pt</supported-locale>
  </locale-config>
  <message-bundle>br.com.internetbanking.messages.messages</message-bundle>
  <resource-bundle>
   <base-name>br.com.internetbanking.messages.messages</base-name>
   <var>msg</var>
  </resource-bundle>
  <!--   <view-handler>br.com.internetbanking.viewhandler.CustomViewHandler</view-handler>   
   <view-handler>br.com.internetbanking.viewhandler.FaceletCustomViewHandler</view-handler> -->
 </application>
</faces-config>
